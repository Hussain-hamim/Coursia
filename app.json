{
  "expo": {
    "name": "Coursia",
    "slug": "bolt-expo-nativewind",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/coursia-logo.png",
    "splash": {
      "image": "./assets/images/coursia-logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "bundleIdentifier": "com.coursesapp.app",
      "supportsTablet": true,
      "statusBar": {
        "barStyle": "light-content"
      },
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "package": "com.coursesapp.app",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/coursia-logo.png",
        "backgroundColor": "#7c6ff5"
      },
      "statusBar": {
        "barStyle": "light-content",
        "backgroundColor": "#7c6ff5"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": ["expo-router", "expo-font", "expo-web-browser"],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "2a1caca0-dce6-425a-9de2-79886c90c2f6"
      }
    }
  }
}
